{
  "opts": {
    "template": "jsdoc/template"
  },
  "plugins": [
    "jsdoc/plugins/events",
    "jsdoc/plugins/typedef_augments",
    "jsdoc/plugins/dot_in_name",
    "node_modules/jsdoc-autoprivate/autoprivate.js",
    "plugins/markdown"
  ],
  "templates": {
    "applicationName": "GeoJS",
    "linenums": true,
    "cleverLinks": true,
    "default": {
      "outputSourceFiles": true
    }
  },
  "docdash": {
    "static": true,
    "sort": true,
    "sectionOrder": [
      "Classes",
      "Modules",
      "Externals",
      "Events",
      "Namespaces",
      "Mixins",
      "Tutorials",
      "Interfaces"
    ],
    "search": true,
    "collapse": true,
    "wrap": false,
    "typedefs": true,
    "removeQuotes": "trim",
    "scopeInOutputPath": false,
    "nameInOutputPath": false,
    "versionInOutputPath": false,
    "menu": {
      "GitHub Repository": {
        "href": "https://github.com/OpenGeoscience/geojs",
        "target":"_blank",
        "class":"menu-item",
        "id":"website_link"
      }
    },
    "openGraph": {
      "title": "GeoJS"
    },
    "meta": {
      "title": "GeoJS"
    }
  }
}
